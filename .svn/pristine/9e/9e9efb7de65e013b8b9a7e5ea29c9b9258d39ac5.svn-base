/* intro9-8*/
*{
	padding:0;
	margin:0;
}
.bg_ditu {
	background-image:  url(/images/intro_map.jpg);
	background-repeat: no-repeat;
	height: 132px;
	width: 928px;
}
9-5 ol, ul {
 list-style-image: none;
 list-style-type: none;
}
img, fieldset {
	border:0;
}
.clear {
	clear: both;
}
.intro_flower {
	width:930px;
	margin-right: auto;
	margin-left: auto;
	background-color: #FFFFFF;
}
.intro_flower_titl {
	background-image:  url(/images/bg_intro_flowers.gif);
	background-repeat: repeat-x;
	height: 30px;
}
.intro_flower_titl_left {
	background-image:  url(/images/bg_intro_001.gif);
	background-repeat: no-repeat;
	background-position: left;
	width: 73px;
	height: 23px;
	color: #ea396e;
	font-size: 14px;
	font-weight: bold;
	padding-top: 7px;
	padding-left: 25px;
	float: left;
}
.intro_flower_titl_right {
	background-image:  url(/images/bg_intro_002.gif);
	background-repeat: no-repeat;
	background-position: right;
	height: 24px;
	width: 137px;
	float: right;
	color: #666666;
	padding-top: 6px;
}
.intro_flower_titl_right a {
	color: #666666;
	text-decoration: underline;
}
.intro_flower_titl_right a:hover {
	color: #FF6600;
	text-decoration: underline;
}
.intro_flower_top {
	background-image:  url(/images/bg_intro_003.gif);
	background-repeat: repeat-y;
	padding-top: 10px;
}
.intro_flower_middle {
	background-repeat: repeat-y;
	padding-right: 10px;
	padding-left: 10px;
	padding-top: 10px;
	padding-bottom: 10px;
	background-image:  url(/images/bg_intro_003.gif);
}
.countr {
	height:auto;
	border-bottom-width: 1px;
	border-bottom-style: dashed;
	border-bottom-color: #969696;
	text-align: left;
	padding-right: 10px;
	padding-left: 10px;
	margin-bottom: 10px;
	padding-bottom: 5px;
}
.countr_left {
	float: left;
	width: 50px;
	background-image:  url(/images/bg_country.gif);
	background-repeat: no-repeat;
	background-position: 0px 0px;
	height: auto;
	font-size: 14px;
	font-weight: bold;
	color: #db5d00;
	padding-top: 3px;
	padding-left: 10px;
	padding-bottom: 2px;
	margin-top: 5px;
}
.countr_right {
	float: left;
	width: 820px;
	padding-left: 10px;
	color: #666666;
	line-height: 26px;
}
.font_c_orange {
	color:#666666;
	line-height:25px;
}
.font_c_orange a {
	color: #666666;
	text-decoration: underline;
}
.font_c_orange a:hover {
	color: #DB5D00;
	text-decoration: underline;
}
.intor_flower_attention {
	height: auto;
	background-image:  url(/images/bg_flower_end.jpg);
	background-repeat: no-repeat;
	background-position: bottom;
	padding-left: 15px;
	border-right-width: 1px;
	border-left-width: 1px;
	border-right-style: solid;
	border-left-style: solid;
	border-right-color: #e6e6e6;
	border-left-color: #e6e6e6;
	padding-bottom: 15px;
}
.intor_flower_attention h1 {
	font-size: 14px;
	color: #666666;
	font-weight: bold;
	float: left;
}
.attention {
	list-style-type: none;
	list-style-position: inside;
	line-height: 22px;
	float: left;
	width: 800px;
	text-align: left;
	padding-left: 10px;
}
.attention li {
	color: #666666;
	list-style-type: decimal;
}
.intro_flower_end {
	background-image:  url(/images/bg_flower_end_001.jpg);
	background-repeat: no-repeat;
	background-position: top;
	height: 9px;
}
