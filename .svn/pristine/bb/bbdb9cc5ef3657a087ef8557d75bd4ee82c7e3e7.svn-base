/* simScroll Style */
.simScroll {position: relative;	float: left;width: 13px;margin-left: -15px;	border-right: solid 1px #BFE8FA}
/* 向上 */
.simScrollUp {height: 14px;	background: url(simScroll_Up.gif) no-repeat}
.simScrollUp_Over {height: 14px;background: url(simScroll_Up_Over.gif) no-repeat}
/* 滚动条 */
.simScrollDragArea {background: url(simScroll_Drag_Area.gif) repeat-y}
.simScrollDrag {background: url(simScroll_Drag.gif) repeat-y}
.simScrollDrag_Over {background: url(simScroll_Drag_Over.gif) repeat-y}
/* 向下 */
.simScrollDown {height: 14px;background: url(simScroll_Down.gif) no-repeat}
.simScrollDown_Over {height: 14px;background: url(simScroll_Down_Over.gif) no-repeat}
.simScrollCont {height: 150px;background: #ECF9FF;border: solid 1px #BFE8FA;padding: 0;overflow: hidden}
